<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="res_company_form_view_inherit_account_daeris" model="ir.ui.view">
        <field name="name">res.company.form.inherit.account.daeris</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='company_registry']" position="after">
                  <field name='ssnid'/>
                  <field name='cnae_code'/>
                  <field name='cnae_name'/>
                  <!--
                  <field name='cnae_coef_it'/>
                  <field name='cnae_coef_ims'/>
                  -->
            </xpath>
        </field>
    </record>
</odoo>